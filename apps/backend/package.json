{
  "name": "@abacus/backend",
  "version": "0.0.1",
  "main": "build/cjs/index.js",
  "types": "build/cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "prebuild": "yarn clean",
    "build": "ts-build ./src --cjs --esm --out-dir ./build",
